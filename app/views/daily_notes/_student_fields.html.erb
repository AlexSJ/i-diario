<tr class="nested-fields">
  <td>
    <%= sequence %>
    <%= f.hidden_field :id %>
  </td>
  <td>
    <%= f.input :student_id, as: :hidden %>
    <%= f.object.student.api_code + ' - ' + f.object.student.to_s %>
  </td>

  <td>
    <%= f.input :note, as: :string, label: false, input_html: { maxlength: 3, style: 'margin-bottom: 0px;'} %>
  </td>
</tr>